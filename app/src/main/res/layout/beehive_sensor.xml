    <TextView
        android:id="@+id/beehiveid"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:textIsSelectable="false"
        android:textSize="20sp"
        android:layout_weight="1"
        ></TextView>

    <TextView
        android:id="@+id/wendu"
        android:layout_width="90dp"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:textIsSelectable="false"
        android:textSize="20sp" />
    <TextView
        android:id="@+id/shidu"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:textIsSelectable="false"
        android:textSize="20sp"
        android:layout_weight="1"
        ></TextView>
    <TextView
        android:id="@+id/dngguang"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:textIsSelectable="false"
        android:textSize="20sp"
        android:layout_weight="1"
        ></TextView>
    <TextView
        android:id="@+id/baojign"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:textColor="#000000"
        android:textIsSelectable="false"
        android:textSize="20sp"
        android:layout_weight="1"
        ></TextView>

</TableRow>